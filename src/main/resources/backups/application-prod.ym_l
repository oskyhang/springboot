#配置端口和项目根路径
server:
  port: 8081
  context-path: /springboot
  
spring:
  datasource:
    username: root
    password: root
    url: jdbc:mysql://localhost:3306/test
    driver-class-name: com.mysql.jdbc.Driver
  jpa:
    properties:
      hibernate:
        ddl-auto: update
        dialect: org.hibernate.dialect.MySQL5InnoDBDialect
    show-sql: true

logging:
  level:
    org:
      springframework:
        web:
          INFO
#设置属性
com:
  neo:
    title: skyhang
    description: hello world
    cnName: 123456

